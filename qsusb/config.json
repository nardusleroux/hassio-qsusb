{
  "name": "QwikSwitch USB Hub",
  "slug": "qsusb",
  "description": "Add-on for the QwikSwitch USB Hub",
  "version": "1.0",
  "startup": "services",
  "boot": "auto",
  "url": "https://github.com/nardusleroux/hassio-qsusb",
  "webui": "http://[HOST]:[PORT:2020]",
  "devices": ["/dev/bus/usb"],
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "ports": {
    "2020/tcp": 2020
  },
  "usb": true,
  "ports_description": {
    "2020/tcp": "QS USB Web UI"
  },
  "hassio_api": true,
  "hassio_role": "default",
  "host_network": true,
  "options": {
    "devices": [
      { "string": "@129b70/rel/Study lamp" },
      { "string": "@16aa30/rel/Kitchen lamp" }
    ]
  },
  "schema": {
    "devices": [
      { "string": "str" }
    ]
  }
}
